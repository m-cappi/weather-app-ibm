{
  "expo": {
    "name": "Nubes Heladas",
    "slug": "nubesheladas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC2F02"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#DC2F02"
      },
      "permissions": [
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.nepha199.nubesheladas"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "MobileApp de Clima para la especializacion en Mobile de Codo a Codo & IBM",
    "githubUrl": "https://github.com/nepha199/weather-app-ibm"
  }
}
